{
    "rewrites": [
        {
            "source": "/flowpetpro/(.*)",
            "destination": "/flowpetpro/index.html"
        },
        {
            "source": "/tutor/(.*)",
            "destination": "/tutor/index.html"
        },
        {
            "source": "/admin/(.*)",
            "destination": "/admin/index.html"
        },
        {
            "source": "/((?!flowpetpro/|tutor/|admin/).*)",
            "destination": "/index.html"
        }
    ]
}